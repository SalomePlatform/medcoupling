# source path
top_srcdir=@top_srcdir@
top_builddir=..
srcdir=@srcdir@
VPATH=.:@srcdir@

SUBDIRS=html MEDMEM

doc:
	@@SETX@; for d in $(SUBDIRS); do 	\
	   (cd $$d && $(MAKE) $@) || exit 1; 	\
	done
clean:
	@@SETX@; for d in $(SUBDIRS); do 	\
	   (cd $$d && $(MAKE) $@) || exit 1; 	\
	done

distclean: clean
	@@SETX@; for d in $(SUBDIRS); do 	\
	   (cd $$d && $(MAKE) $@) || exit 1; 	\
	done

install:
	@@SETX@; for d in $(SUBDIRS); do 	\
	   (cd $$d && $(MAKE) $@) || exit 1; 	\
	done
